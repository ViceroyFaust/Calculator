{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-pedantic-errors",
                "-Wall",
                "-Weffc++",
                "-Wextra",
                "-Wsign-conversion",
                "-std=c++20",
                "-I/usr/lib/wx/include/gtk2-unicode-3.0",
                "-I/usr/include/wx-3.0",
                "-D_FILE_OFFSET_BITS=64",
                "-DWXUSINGDLL",
                "-D__WXGTK__",
                "-pthread",
                "-lwx_gtk2u_xrc-3.0",
                "-lwx_gtk2u_html-3.0",
                "-lwx_gtk2u_qa-3.0",
                "-lwx_gtk2u_adv-3.0",
                "-lwx_gtk2u_core-3.0",
                "-lwx_baseu_xml-3.0",
                "-lwx_baseu_net-3.0",
                "-lwx_baseu-3.0 "
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}